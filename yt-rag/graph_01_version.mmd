---
config:
  flowchart:
    curve: linear
---
graph TD;
	__start__([<p>__start__</p>]):::first
	supervisor(supervisor)
	planner(planner)
	chat(chat)
	fallback(fallback)
	rag(rag)
	executioner(executioner)
	__end__([<p>__end__</p>]):::last
	__start__ --> supervisor;
	executioner -. &nbsp;stop&nbsp; .-> __end__;
	executioner -.-> chat;
	executioner -.-> fallback;
	executioner -.-> rag;
	planner --> executioner;
	rag --> executioner;
	supervisor -.-> chat;
	supervisor -.-> fallback;
	supervisor -.-> planner;
	chat --> __end__;
	fallback --> __end__;
	classDef default fill:#f2f0ff,line-height:1.2
	classDef first fill-opacity:0
	classDef last fill:#bfb6fc
